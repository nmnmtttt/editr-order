{
  "manifest_version": 2,
  "name": "WEB editor ",
  "short_name": "WEBeditor",
  "version": "0.0.1",
  "description": "网页编辑",
  "icons": {
    "16": "static/icon/logo.png",
    "48": "static/icon/logo.png",
    "128": "static/icon/logo.png"
  },
  "browser_action": {
    "default_icon": "static/icon/logo.png",
    "default_title": "WEB editor"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": ["inject.js", "static/*", "css/*", "iamges/*"],
  "devtools_page": "static/devtool.html",
  "permissions": [
    "tabs",
    "activeTab",
    "background",
    "webRequest",
    "contextMenus",
    "downloads",
    "webNavigation",
    "storage",
    "debugger",
    "webRequestBlocking",
    "unlimitedStorage",
    "cookies",
    "management",
    "superbrowser",
    "<all_urls>"
  ],
  "externally_connectable": {
    "ids": ["*"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
